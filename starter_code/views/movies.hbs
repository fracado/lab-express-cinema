<h1>Ironhack Cinema</h1>
<p>Click on the movie to check the showtimes!</p>
<div class="movies-container">
  {{#each movies}}
  <div class="card">
        <img src="{{this.image}}" alt="Movie-{{this.title}}"><br>
        <span>{{this.title}}</span><br>
        <a href="/movies/{{this._id}}">See more</a>
  </div>
  {{/each}}
</div>